@model RealEstate.Models.Contact
@{
    ViewBag.Title = "ContactDerails";
    Layout = "~/Views/Shared/AdminLayoutPage.cshtml";
}
@using System.Data
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h5 class="m-0 font-weight-bold text-primary">Contact Details</h5>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <div id="dataTable_wrapper" class="dataTables_wrapper dt-bootstrap4">
                <div class="row">
                    <div class="col-sm-12">
                        <table class="table table-bordered dataTable" width="100%" cellspacing="0" role="grid" aria-describedby="dataTable_info" style="width: 100%;">
                            <thead>
                                <tr role="row">
                                    <th>Sr. No.</th>
                                    <th>Name</th>
                                    <th>Email</th>
                                    <th>Subject</th>
                                    <th>Mobile No</th>
                                    <th>Message</th>
                                    <th>Date</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr role="row">
                                    @if (Model._dt != null && Model._dt.Rows.Count > 0)
                                    {
                                        int i = 0;
                                        foreach (DataRow item in Model._dt.Rows)
                                        {
                                            i++;

                                        <tr>
                                            <td>@i </td>

                                            <td>@item["Name"]</td>
                                            <td>@item["Email"]</td>
                                            <td>@item["Subject"]</td>
                                            <td>@item["Mobile"]</td>
                                            
                                            <td>@item["Message"]</td>
                                            <td>@item["EntryDate"]</td>
                                            .



                                        </tr>
                                    }
                                }


                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

