	<script>


		function _prop_enquire_modal(Id, propName) {
			$('#propEnquire').modal('show');
			$('#propName').html(propName);
			$('#PlotId').val(Id);
		}

		function sendEnquiry() {
			var PlotId = $('#PlotId').val();
			var Name = $('#Name').val();
			var Email = $('#Email').val();
			var Phone = $('#Phone').val();
			var Message = $('#Message').val();

			if (Name == '' || Name == 'NaN' || Name == null || Name == undefined) {
				fireModal('Please enter your name!');
				return false;
			}
			if (Phone == '' || Phone == 'NaN' || Phone == null || Phone == undefined) {
				fireModal('Please enter your phone!');
				return false;
			}
			if (Email == '' || Email == 'NaN' || Email == null || Email == undefined) {
				fireModal('Please enter your email!');
				return false;
			}
			var dataObj = { PlotId: PlotId, Name: Name, Email: Email, Phone: Phone, Message: Message };

			$.post('/home/_saveEnquiry', dataObj, function (data) {
				//data = eval(data);
				if (data != '0') {
					Swal.fire(
						'Good Job',
						data,
						'success'
					);
					$('#propEnquire').modal('hide');
					clrEnqForm();
				}
				else {
					Swal.fire(
						'Exception',
						'Server error occured!',
						'error'
					);
					$('#propEnquire').modal('hide');
					clrEnqForm();
				}
			});
		}

		function fireModal(title) {
			Swal.fire({
				position: 'center',
				icon: 'info',
				title: title,
				showConfirmButton: true,
				timer: 1500
			})
		}

		function clrEnqForm() {
			$('#PlotId').val('');
			$('#Name').val('');
			$('#Email').val('');
			$('#Phone').val('');
			$('#Message').val('');
		}



	</script>

