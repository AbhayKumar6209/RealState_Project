	<!-- Modal -->
	<div class="modal fade" id="propEnquire" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="propEnquireLabel" aria-hidden="true">
		<div class="modal-dialog" role="document">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title">Plot Enquiry: <span id="propName" class="text-danger"></span></h5>
					<button type="button" class="close btn btn-sm" style="font-size: 22px;" onclick="$('#propEnquire').modal('hide');">
